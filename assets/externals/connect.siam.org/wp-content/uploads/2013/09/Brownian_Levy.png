<script type="text/javascript">
    function MobileSearch() {
        document.location.href = "/SearchResults.aspx?Search=" + $("#mobilesearch").val();
    }

/* Custom "cleanup" code */
  jQuery(function($) { 
    $(document).ready( function() {
      /* add category name as class to category links
         and article tile to apply custom styles */
      $(".article").each(function(index, article){
        // get main (first) category link
        var mainCatLink = $(article).find(".article-category a").eq(0);
        // add basic category button style
        mainCatLink.addClass("btn-category");
        if(mainCatLink != null && mainCatLink.length > 0){
          // get name of category for class
          var mainCatClass = $(mainCatLink).attr('class').split(' ')[0];
          $(article).addClass(mainCatClass);
        }
        // hide all category links except for first
        $(article).find(".article-category a").not(mainCatLink).hide();

        // if category is not top-level, show top-level text instead
        if(mainCatLink.text() != "Happening Now" ||
           mainCatLink.text() != "Get Involved" ||
           mainCatLink.text() != "Research" ||
           mainCatLink.text() != "Careers"){
          $(mainCatLink).text(GetTopLevelCategory($(mainCatLink).text()));
        }
      });

      // hide empty "non-article" containers
      $(".sinews-tile-non-article").each(function(index, item){
        if($(item).text().trim() == "")
          $(item).hide();
      });

      // show only summaries that have real content
      $(".EasyDNNnewsArticleSummary").each(function(){
        if($(this).text().indexOf("[EasyDNNnews:Article:Limit:150]") <= -1){
            $(this).css("display", "block");
        }
      });

      //siam connect redirect notification
      if ((window.location.href.search(/connectredirect.true/) !== -1) ||
        (window.location.href.search(/blogsredirect.true/) !== -1)) {
          history.replaceState(null, null, '/');
          $('#siamConnectRedirectDiv').slideDown();
          $('#siamConnectRedirectDiv').click(function() {
            $('#siamConnectRedirectDiv').slideUp();
          });
          setTimeout(function() {
            $('#siamConnectRedirectDiv').slideUp();
          }, 10000);
      }

    });

    $("<li><div class=\"mobilemenu_left\"><a href=\"/subscribe\" class=\"btn-subscribe\">Subscribe</a></div><div class=\"mobilemenu_right\"><input type=\"text\" id=\"mobilesearch\" /> <a title=\"Search\" href=\"javascript:MobileSearch()\" id=\"#MobileSearchIcon\" class=\"icon-search fa fa-search\"></a></div><div class=\"mobilemenu_bottom\"></div></li>").insertBefore($("#TreeMenu_ul").children().first());

   

    function GetTopLevelCategory(subCategory){
      switch(subCategory){
        case "Programs and Publications":
        case "Awards and Recognition":
        case "Science Policy":
        case "Unwrapped":
        case "Announcements":
          return "Happening Now";
        break;
        case "Conferences":
        case "Membership":
        case "Prize Nominations":
          return "Get Involved";
        break;
        case "Insights":
        case "Research Nuggets":
        case "Commentary":
          return "Research";
        break;
        case "Students and Education":
        case "Professional Development":
        case "Got Funding":
          return "Careers";
        break;
        default: return subCategory;
      }
    }
  });
</script>
